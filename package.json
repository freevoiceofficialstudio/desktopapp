
{
  "name": "free-voice-desktop",
  "version": "3.0.2",
  "description": "Professional AI Voice Changer",
  "main": "electron-main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "author": "Subhan Ahmad",
  "license": "Proprietary",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.freevoice.studio",
    "productName": "Free Voice",
    "asar": true,
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
